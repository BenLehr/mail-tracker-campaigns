{
    "name": "benlehr/mail-tracker",
    "type": "library",
    "description": "Logs and tracks all outgoing emails from Laravel",
    "keywords": [
        "benlehr",
        "MailTracker"
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/benlehr/MailTracker",
    "license": "MIT",
    "authors": [{
        "name": "Benjamin Lehr",
        "email": "benlehr@trio-group.de",
        "homepage": "https://trio-group.de",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "~7.0|~8.0",
        "guzzlehttp/guzzle": "^5.3.1|^6.2.1|^7.0",
        "aws/aws-php-sns-message-validator": "^1.1",
        "aws/aws-sdk-php": "^3.133"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "orchestra/testbench": "~5.0|~6.0",
        "mockery/mockery": "^1.1"
    },
    "suggest": {
        "fedeisas/laravel-mail-css-inliner": "Automatically inlines CSS into all outgoing mail."
    },
    "autoload": {
        "psr-4": {
            "benlehr\\MailTracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "benlehr\\MailTracker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "benlehr\\MailTracker\\MailTrackerServiceProvider"
            ]
        }
    }
}
